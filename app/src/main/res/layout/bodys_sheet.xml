            android:background="@color/colorAccent"
            android:orientation="horizontal">

            <com.google.android.material.button.MaterialButton
                android:id="@+id/ART"
                style="@style/ButtonBoDySCategory"
                android:text="@string/ART" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="300dp"
                android:layout_height="match_parent">

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/ART1"
                    style="@style/ButtonBoDyS"
                    android:text="@string/ART1"
                    android:layout_width="0dp"
                    app:layout_constraintWidth_percent="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/ART2"
                    style="@style/ButtonBoDyS"
                    android:layout_width="0dp"
                    android:layout_marginStart="1dp"
                    android:text="@string/ART2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/ART1"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/ART4"
                    app:layout_constraintWidth_percent="0.5" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/ART3"
                    style="@style/ButtonBoDyS"
                    android:text="@string/ART3"
                    android:layout_marginTop="1dp"
                    android:layout_width="0dp"
                    app:layout_constraintWidth_percent="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ART1" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/ART4"
                    style="@style/ButtonBoDyS"
                    android:text="@string/ART4"
                    android:layout_marginTop="1dp"
                    android:layout_marginStart="1dp"
                    android:layout_width="0dp"
                    app:layout_constraintWidth_percent="0.5"
                    app:layout_constraintStart_toEndOf="@+id/ART3"
                    app:layout_constraintTop_toBottomOf="@+id/ART2" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/ART5"
                    style="@style/ButtonBoDyS"
                    android:text="@string/ART5"
                    android:layout_marginTop="1dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ART3" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

    </androidx.cardview.widget.CardView>

