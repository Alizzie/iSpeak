                <com.google.android.material.button.MaterialButton
                    android:id="@+id/STS1"
                    style="@style/ButtonBoDyS"
                    android:text="@string/STS1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/STS2"
                    style="@style/ButtonBoDyS"
                    android:text="@string/STS2"
                    android:layout_marginTop="1dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/STS1" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/STS3"
                    style="@style/ButtonBoDyS"
                    android:text="@string/STS3"
                    android:layout_marginTop="1dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/STS2" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/STS4"
                    style="@style/ButtonBoDyS"
                    android:text="@string/STS4"
                    android:layout_marginTop="1dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/STS3" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/STS5"
                    style="@style/ButtonBoDyS"
                    android:text="@string/STS5"
                    android:layout_marginTop="1dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/STS4" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

